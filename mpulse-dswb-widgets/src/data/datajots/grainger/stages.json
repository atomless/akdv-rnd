{
    "name": "Demo Session Data",
    "description": "Session data over one day for an ecommerce client",
    "theme": "default",
    "data_format": "tsv",
    "stages": [
        {
            "id": 1,
            "anchortext": "start",
            "description": "shows all available data in a cloud",
            "displaytext": "This cluster represents 5,000 sessions on Grainger.com on Thu Nov 23.",
            "dimensions": [
            ]
        },
        {
            "id": 2,
            "anchortext": "alloptions",
            "description": "shows all available options",
            "displaytext": "By COUNTRYCODE",
            "filters": [
                {
                    "DEVICETYPENAME": "desktop"
                }
            ],
            "dimensions": [
                { 
                    "name": "axis_y",
                    "properties" : {
                        "by": "COUNTRYCODE",
                        "label": "Country Code"
                    }
                },
                { 
                    "name": "color",
                    "properties" : {
                        "by": "DEVICETYPENAME",
                        "palette": ["hsl(220,100%,50%)", "hsl(190,100%,50%)", "hsl(120,100%,50%)", "hsl(80,100%,50%)", "hsl(40,100%,50%)", "hsl(350,100%,50%)"]
                    }
                },
                { 
                    "name": "opacity",
                    "properties" : {
                        "by": "SESSIONDURATION",
                        "reverse": false
                    }
                }
            ]
        },
        {
            "id": 3,
            "anchortext": "devicetypes",
            "description": "cluster all sessions by device type name",
            "displaytext": "87% of sessions were on Desktop<br/>10% of sessions were on Smartphone<br/>3% were on Tablet.",
            "dimensions": [
                {
                    "name": "x",
                    "properties": {
                        "by": "DEVICETYPENAME"
                    }
                },
                { 
                    "name": "color",
                    "properties" : {
                        "by": "DEVICETYPENAME",
                        "palette": ["hsl(220,100%,50%)", "hsl(190,100%,50%)", "hsl(120,100%,50%)", "hsl(80,100%,50%)", "hsl(40,100%,50%)", "hsl(350,100%,50%)"]
                    }
                }
            ]
        },
        {
            "id": 4,
            "anchortext": "countries",
            "description": "cluster all sessions by countries",
            "displaytext": "97% of sessions were from The United States.",
            "dimensions": [
                {
                    "name": "x",
                    "properties": {
                        "by": "COUNTRYCODE",
                         "palette": ["hsl(220,100%,50%)", "hsl(190,100%,50%)", "hsl(120,100%,50%)", "hsl(80,100%,50%)", "hsl(40,100%,50%)", "hsl(350,100%,50%)"]
                    }
                },
                {
                    "name": "x",
                    "properties": {
                        "by": "COUNTRYCODE"
                    }
                },
                {
                    "name": "color",
                    "properties": {
                        "by": "COUNTRYCODE",
                        "palette": ["hsl(220,100%,50%)", "hsl(190,100%,50%)", "hsl(120,100%,50%)", "hsl(80,100%,50%)", "hsl(40,100%,50%)", "hsl(350,100%,50%)"]
                    }
                }
            ]
        },
        {
            "id": 5,
            "anchortext": "countries",
            "description": "vertical distribution of performance, with #1 country highlighted",
            "displaytext": "Sessions from The United States averaged 3.9 seconds",
            "dimensions": [
                {   
                    "name": "color",
                    "properties": {
                        "by": "COUNTRYCODE",
                        "palette": ["hsl(220,100%,50%)", "hsl(190,100%,50%)", "hsl(120,100%,50%)", "hsl(80,100%,50%)", "hsl(40,100%,50%)", "hsl(350,100%,50%)"]
                    }
                },
                {
                    "name": "y",
                    "properties": {
                        "by": "PAGELOADTIME"
                    }
                }
            ]
        },
        {
            "id": 6,
            "anchortext": "conversion",
            "description": "sessions highlighted by conversion",
            "displaytext": "2.9% of sessions converted",
            "dimensions": [
                {   
                    "name": "color",
                    "properties": {
                        "by": "CONVERSION",
                        "palette": ["hsl(240,100%,90%)", "hsl(120,100%,50%)"]
                    }
                },
                {   
                    "name": "x",
                    "properties": {
                        "by": "CONVERSION"
                    }
                }
            ]
        },
        {
            "id": 7,
            "anchortext": "conversion_speed",
            "description": "vertical distribution of performance, with #1 country highlighted",
            "displaytext": "Converted sessions were, on average, 27% faster than non-converted sessions",
            "dimensions": [
                {   
                    "name": "color",
                    "properties": {
                        "by": "CONVERSION",
                        "palette": ["hsl(240,100%,90%)", "hsl(120,100%,50%)"]
                    }
                },
                {
                    "name": "x",
                    "properties": {
                        "by": "PAGELOADTIME"
                    }
                }
            ]
        },
        {
            "id": 8,
            "anchortext": "conversion_sessionlength",
            "description": "vertical distribution of performance, with #1 country highlighted",
            "displaytext": "Converted sessions were 9 pages long on average, compared with 3 pages for non-converted sessions",
            "dimensions": [
                {   
                    "name": "color",
                    "properties": {
                        "by": "CONVERSION",
                        "palette": ["hsl(240,100%,90%)", "hsl(120,100%,50%)"]
                    }
                },
                {
                    "name": "size",
                    "properties": {
                        "by": "SESSIONLENGTH"
                    }
                }
            ]
        }
    ]
}