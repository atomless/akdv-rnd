{
    "name": "Cancer rates & survival",
    "description": "5 year survival rates from 2010 - 2015",
    "theme": "default",
    "data_format": "tsv",
    "stages": [
        {
            "id": 1,
            "anchortext": "start",
            "description": "shows all available data in a cloud",
            "displaytext": "This cluster represents all tweets by @realDonaldTrump in 2017",
            "disableclustering": true,
            "dimensions": []
        },
        {
            "id": 2,
            "anchortext": "start",
            "description": "shows all available data in a cloud",
            "displaytext": "Distributed by time on the x-axis",
            "dimensions": [
                {
                    "name": "x",
                    "properties": {
                        "by": "created_at"
                    }
                }
            ]
        },
        {
            "id": 3,
            "anchortext": "start",
            "description": "shows all available data in a cloud",
            "displaytext": "Distributed by tweet length on the y-axis",
            "dimensions": [
                {
                    "name": "x",
                    "properties": {
                        "by": "created_at"
                    }
                },
                {
                    "name": "y",
                    "properties": {
                        "by": "length",
                        "reverse": true
                    }
                }
            ]
        },
        {
            "id": 4,
            "anchortext": "start",
            "description": "shows all available data in a cloud",
            "displaytext": "Sized by favorite count",
            "dimensions": [
                {
                    "name": "x",
                    "properties": {
                        "by": "created_at"
                    }
                },
                {
                    "name": "y",
                    "properties": {
                        "by": "length"
                    }
                },
                {
                    "name": "radius",
                    "properties": {
                        "by": "favourite_count"
                    }
                }
            ]
        },
        {
            "id": 5,
            "anchortext": "start",
            "description": "shows all available data in a cloud",
            "displaytext": "Colored by RTs",
            "dimensions": [
                {
                    "name": "x",
                    "properties": {
                        "by": "created_at"
                    }
                },
                {
                    "name": "y",
                    "properties": {
                        "by": "length"
                    }
                },
                {
                    "name": "color",
                    "properties": {
                        "by": "retweet_count",
                        "palette": ["hsl(240,100%,60%)", "hsl(120,100%,40%)"]
                    }
                },
                {
                    "name": "radius",
                    "properties": {
                        "by": "favourite_count"
                    }
                }
            ]
        }
    ]
}