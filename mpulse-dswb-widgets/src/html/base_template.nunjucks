<!DOCTYPE html>
{{ copyright_and_versioning|safe }}
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>{{author_info|safe}}</title>  

    <script src="../javascript/lib/akdv.boomr.js"></script>
    {% include "partials/boomr_snippet.nunjucks" %}

    {% block additional_head_scripts %} {% endblock %}

    <style>{{ critical_path_css }}</style>

    <link rel="shortcut icon" type="image/png" href="../images/favicon.ico">
    
    <link rel="stylesheet" type="text/css" href="../css/themes/dswb_dark_theme.css" media="none" class="css-theme" id="dswb-dark-theme-css">
    <link rel="stylesheet" type="text/css" href="../css/themes/dswb_light_theme.css" media="all" class="css-theme" id="dswb-light-theme-css">
    <link rel="stylesheet" type="text/css" href="../css/akdv.css" media="all">

    {% block additional_css %} {% endblock %}

    {% include "partials/loadcss_scripts.nunjucks" %}

    {% include "partials/base_font_stacks.nunjucks" %}
  </head>

  <body class="{% block body_classes %} {% endblock %}" data-render-complete="false"
        data-theme-initialized="false"
        data-software-version="{{version_info|safe}}"
        data-publication-date="{{date_info|safe}}" style="visibility: hidden;">

    {% include "partials/widget_status_notifier.nunjucks" %}

    {# You write html for this block in your chart html files (see example_chart.nunjucks) #}
    {% block chart %} {% endblock %}

    {% block annotation_layer %} {% endblock %}

    {% include "partials/svg_spinner.nunjucks" %}

    {#
    <!-- ******************************** --> 
    <!-- SCRIPTS                          -->
    <!-- ******************************** --> 
    <!-- REQUIRED VENDOR SCRIPTS FOR IE11 -->
    #}

    {% include "partials/ie11_scripts.nunjucks" %}

    {#<!-- REQUIRED VENDOR SCRIPTS -->#}
    <script src="../javascript/vendor/babel.polyfill.min.js"></script>
    <script src="../javascript/vendor/thenBy.min.js"></script>
    <script src="../javascript/vendor/jquery.slim.min.js"></script>
    <script src="../javascript/vendor/simple-scrollbar.min.js"></script>
    <script src="../javascript/vendor/moment.min.js"></script>
    <script src="../javascript/vendor/numeral.min.js"></script>
    <script src="../javascript/vendor/moment-timezone-with-data.min.js"></script>
    <script src="../javascript/vendor/usertiming.min.js"></script>
    <script src="../javascript/vendor/smooth_scroll_polyfill.js"></script>
    <script src="../javascript/vendor/xmltojson.min.js"></script>
    <script src="../javascript/vendor/markdown-it.min.js"></script>
    {#<!-- OPTIONAL ADDITIONAL VENDOR SCRIPTS -->#}
    {% block additional_vendor_scripts %} {% endblock %}
    {#<!-- REQUIRED IN HOUSE SCRIPTS -->#}
    <script src="../javascript/lib/_log.js"></script>
    <script src="../javascript/lib/functional.js"></script>
    <script src="../javascript/lib/exceptions.js"></script>
    <script src="../javascript/lib/akdv.js"></script>
    <script src="../javascript/lib/akdv.storage.js"></script>    
    <script src="../javascript/lib/akdv.sloFunProxy.js"></script>
    <script src="../javascript/lib/akdv.utils.js"></script>
    <script src="../javascript/lib/akdv.utils_env.js"></script>
    <script src="../javascript/lib/akdv.utils_obj.js"></script>
    <script src="../javascript/lib/akdv.utils_string.js"></script>
    <script src="../javascript/lib/akdv.utils_event.js"></script>
    <script src="../javascript/lib/akdv.utils_dom.js"></script>
    <script src="../javascript/lib/akdv.loading_spinners.js"></script>
    <script src="../javascript/lib/akdv.utils_lang.js"></script>
    <script src="../javascript/lib/akdv.utils_math.js"></script>
    <script src="../javascript/lib/akdv.utils_color.js"></script>
    <script src="../javascript/lib/akdv.utils_css.js"></script>
    <script src="../javascript/lib/akdv.utils_data.js"></script>
    <script src="../javascript/lib/akdv.utils_topology.js"></script>
    <script src="../javascript/lib/akdv.utils_time.js"></script>
    <script src="../javascript/lib/vega_expressions.js"></script>
    <script src="../javascript/lib/akdv.validate.js"></script>
    <script src="../javascript/lib/akdv.themes.js"></script>
    <script src="../javascript/lib/akdv.renderCompleteWatch.js"></script>
    <script src="../javascript/lib/status_factory.js"></script>
    <script src="../javascript/lib/status_history_factory.js"></script>
    <script src="../javascript/lib/akdv.statusNotifier.js"></script>
    <script src="../javascript/lib/akdv.statusHistory.js"></script>
    <script src="../javascript/lib/akdv.performance.js"></script>
    <script src="../javascript/lib/akdv.postMessageAPI.js"></script>
    <script src="../javascript/lib/akdv.mpulse.widget_filter_base_types.js"></script>
    <script src="../javascript/lib/akdv.xmlHTTP.js"></script>
    <script src="../javascript/components/component_factory.js"></script>
    <script src="../javascript/components/annotation_bubble_factory.js"></script>
    <script src="../javascript/components/tooltip_factory.js"></script>
    <script src="../javascript/lib/akdv.tooltips.js"></script>
    <script src="../javascript/lib/akdv.icons.js"></script>
    <script src="../javascript/lib/datasource_factory.js"></script>
    <script src="../javascript/lib/datasource_file.js"></script>
    <script src="../javascript/lib/data_provider_factory.js"></script>
    <script src="../javascript/lib/akdv.mpulse.js"></script>
    <script src="../javascript/lib/socket_factory.js"></script>
    <script src="../javascript/lib/dswb_server_interface_factory.js"></script>
    <script src="../javascript/lib/dswb_session_factory.js"></script>
    <script src="../javascript/lib/datasource_dswb.js"></script>
    <script src="../javascript/index.js"></script>
    {#<!-- SPECIFIC IN HOUSE CHART SCRIPTS -->#}
    <script>
    if (/Trident.*rv[ :]*11\./.test(navigator.userAgent)) {
        let head  = document.getElementsByTagName('head')[0];
        let link  = document.createElement('link');
        link.rel  = 'stylesheet';
        link.type = 'text/css';
        link.href = '../css/ie11/_prefix_override.css';
        link.media = 'all';
        head.appendChild(link);

        let script = document.createElement('script');
        script.src = '../javascript/IE11/color_map.min.js';
        let ref = document.body.querySelector('script');
        ref.parentNode.insertBefore(script, ref);
    }</script>
    {% block chart_scripts %} {% endblock %}
  </body>

</html>