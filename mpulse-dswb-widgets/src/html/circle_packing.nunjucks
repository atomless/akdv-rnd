{% extends "base_template.nunjucks" %}

{% block additional_css %}
    <link href="../css/components/_filters.css" rel="stylesheet" type="text/css">
    <link href="../css/components/_custom_number_input.css" rel="stylesheet" type="text/css">
    <link href="../css/components/_reorderable_list.css" rel="stylesheet" type="text/css">
    <link href="../css/charts/single_page_base_template.css" rel="stylesheet" type="text/css">
    <link href="../css/charts/circle_packing.css" rel="stylesheet" type="text/css">
{% endblock %}


{% block chart %}
    <article>

        <section id="analysis">
            <header>
                <ul id="active-filters" class="active-filter-list"></ul>
            </header>
            <div id="circle-packing" class="chart"></div>
        </section>

    </article>
{% endblock %}


{% block additional_vendor_scripts %}
    <script src="../javascript/vendor/d3.min.js"></script>
    <script src="../javascript/vendor/d3-annotation.min.js"></script>
    <script src="../javascript/vendor/crossfilter.min.js"></script>
{% endblock %}


{% block chart_scripts %}
    <script src="../javascript/lib/akdv.utils_hierarchy.js"></script>
    <script src="../javascript/components/custom_number_input_factory.js"></script>
    <script src="../javascript/components/chart_filter_button_factory.js"></script>
    <script src="../javascript/components/reorderable_list_item_factory.js"></script>
    <script src="../javascript/components/reorderable_list_factory.js"></script>
    <script src="../javascript/components/axis_color_factory.js"></script>
    <script src="../javascript/components/axis_factory.js"></script>
    <script src="../javascript/components/color_legend_factory.js"></script>
    <script src="../javascript/components/annotation_bubble_factory.js"></script>
    <script src="../javascript/components/contextmenu_factory.js"></script>
    <script src="../javascript/components/component_factory.js"></script>
    <script src="../javascript/d3_reusable_charts/d3_circle_packing_plot.js"></script>
    <script src="../javascript/charts/circle_packing.js"></script>
{% endblock %}