<!DOCTYPE html>
{{ copyright_and_versioning|safe }}
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>{{author_info|safe}}</title>  

    {% include "partials/boomr_snippet.nunjucks" %}
    {% block additional_head_scripts %} {% endblock %}

    <link rel="shortcut icon" type="image/png" href="../images/favicon.ico">
    <link rel="stylesheet" type="text/css" href="../css/themes/dswb_dark_theme.css" media="all" class="css-theme" id="dswb-dark-theme-css">
    <link rel="stylesheet" type="text/css" href="../css/themes/dswb_light_theme.css" media="none" class="css-theme" id="dswb-light-theme-css">
    <link rel="stylesheet" type="text/css" href="../css/akdv.css" media="screen">    
    {% include "partials/base_font_stacks.nunjucks" %}
    {% block additional_css %} {% endblock %}
  </head>

  <body data-render-complete="false"
        data-software-version="{{version_info|safe}}"
        data-publication-date="{{date_info|safe}}">

    {% include "partials/widget_status_notifier.nunjucks" %}

    {# You write html for this block in your chart html files (see example_chart.nunjucks) #}
    {% block chart %} {% endblock %}

    <!-- SVG SPINNING LOADER TEMPLATE (needs to be included inline otherwise the animation fails to work) -->
    <svg id="loading-spinner-template" class="loading-spinner" height="50" width="50" viewBox="0 0 50 50">
      <circle class="loading-spinner-path-bg" cx="25" cy="25" r="20" fill="none"></circle>
      <circle class="loading-spinner-path" cx="25" cy="25" r="20" fill="none"></circle>
    </svg>

    <!-- ******************************** --> 
    <!-- SCRIPTS                          -->
    <!-- ******************************** --> 
    <!-- REQUIRED VENDOR SCRIPTS -->
    <script src="../javascript/vendor/babel.polyfill.min.js"></script>
    <script src="../javascript/vendor/thenBy.min.js"></script>
    <script src="../javascript/vendor/promise.min.js"></script>
    <script src="../javascript/vendor/jquery.slim.min.js"></script>
    <script src="../javascript/vendor/simple-scrollbar.min.js"></script>
    <script src="../javascript/vendor/fetch.js"></script>
    <script src="../javascript/vendor/moment.min.js"></script>
    <script src="../javascript/vendor/numeral.min.js"></script>
    <script src="../javascript/vendor/moment-timezone-with-data.min.js"></script>
    <script src="../javascript/vendor/usertiming.min.js"></script>
    <script src="../javascript/vendor/smooth_scroll_polyfill.js"></script>
    <!-- OPTIONAL ADDITIONAL VENDOR SCRIPTS -->
    {% block additional_vendor_scripts %} {% endblock %}
    <!-- REQUIRED IN HOUSE SCRIPTS -->
    <script src="../javascript/lib/exceptions.js"></script>
    <script src="../javascript/lib/akdv.js"></script>
    <script src="../javascript/lib/akdv.loading_spinner.js"></script>
    <script src="../javascript/lib/akdv.sloFunProxy.js"></script>
    <script src="../javascript/lib/akdv.utils.js"></script>
    <script src="../javascript/lib/akdv.utils_lang.js"></script>
    <script src="../javascript/lib/akdv.utils_math.js"></script>
    <script src="../javascript/lib/akdv.utils_obj.js"></script>
    <script src="../javascript/lib/akdv.utils_color.js"></script>
    <script src="../javascript/lib/akdv.utils_css.js"></script>
    <script src="../javascript/lib/akdv.utils_data.js"></script>
    <script src="../javascript/lib/akdv.utils_dom.js"></script>
    <script src="../javascript/lib/akdv.utils_env.js"></script>
    <script src="../javascript/lib/akdv.utils_event.js"></script>
    <script src="../javascript/lib/akdv.utils_string.js"></script>
    <script src="../javascript/lib/akdv.utils_topology.js"></script>
    <script src="../javascript/lib/akdv.validate.js"></script>
    <script src="../javascript/lib/_log.js"></script>
    <script src="../javascript/lib/akdv.themes.js"></script>
    <script src="../javascript/lib/akdv.performance.js"></script>
    <script src="../javascript/lib/akdv.boomr.js"></script>
    <script src="../javascript/lib/akdv.storage.js"></script>
    <script src="../javascript/lib/akdv.postMessageAPI.js"></script>
    <script src="../javascript/lib/akdv.xmlHTTP.js"></script>
    <script src="../javascript/lib/datasource_dswb.js"></script>
    <script src="../javascript/lib/akdv.widgetStatusNotifier.js"></script>
    <!-- SPECIFIC IN HOUSE CHART SCRIPTS -->
    {% block chart_scripts %} {% endblock %}
  </body>

</html>