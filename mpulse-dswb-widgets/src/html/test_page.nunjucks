{% extends "base_template.nunjucks" %}


{% block chart %}
<h1>TEST PAGE</h1>
{% endblock %}


{% block additional_vendor_scripts %}{% endblock %}


{% block chart_scripts %}
    <script>
        // Leave this here please don't even try to move me.  You will be wasting your time!
        $(window).on('init-charts.akdv', function(event) {

            document.body.setAttribute('data-akdv-initialized', true);
        });

        $(window).on('result.data', function(e, data) {

            document.body.setAttribute('data-data-loaded', true);
            window.data = data;
        });
    </script>
{% endblock %}

{% block annotation_layer %}
    <div id="annotation-layer" class="annotation-layer"></div>
{% endblock %}